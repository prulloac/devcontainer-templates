{
    "id": "go_typescript",
    "version": "1.1.0",
    "name": "Go & Typescript",
    "description": "A Template to build fullstack applications with both golang and javascript/typescript stacks (contains Bun)",
    "documentationURL": "https://github.com/prulloac/devcontainer-templates/tree/main/src/go_typescript",
    "licenseURL": "https://github.com/prulloac/devcontainer-templates/blob/main/LICENSE",
    "options": {
        "imageVariant": {
            "type": "string",
            "description": "Debian version (use bullseye on local arm64/Apple Silicon):",
            "proposals": [
                "bullseye",
                "buster",
                "bookworm",
                "ubuntu-24.04",
                "ubuntu-22.04"
            ],
            "default": "bookworm"
        },
        "goVersion": {
            "type": "string",
            "description": "Golang version to install",
            "default": "latest"
        },
        "golangciLintVersion": {
            "type": "string",
            "description": "Golangci-lint version to install",
            "default": "latest"
        }
    },
    "platforms": [
        "Any"
    ]
}
