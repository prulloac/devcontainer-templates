{
    "id": "rust_typescript",
    "version": "1.1.0",
    "name": "Rust & Typescript",
    "description": "A Template to build fullstack applications with both rust and javascript/typescript stacks (contains Bun)",
    "documentationURL": "https://github.com/prulloac/devcontainer-templates/tree/main/src/rust_typescript",
    "licenseURL": "https://github.com/prulloac/devcontainer-templates/blob/main/LICENSE",
    "options": {
        "imageVariant": {
            "type": "string",
            "description": "Debian version (use bullseye on local arm64/Apple Silicon):",
            "proposals": [
                "bullseye",
                "buster",
                "bookworm",
                "ubuntu-24.04",
                "ubuntu-22.04"
            ],
            "default": "bookworm"
        },
        "rustVersion": {
            "type": "string",
            "description": "Rust version to install",
            "default": "latest"
        },
        "rustProfile": {
            "type": "string",
            "description": "Rust profile to install",
            "proposals": [
                "minimal",
                "default",
                "complete"
            ],
            "default": "default"
        }
    },
    "platforms": [
        "Any"
    ]
}
